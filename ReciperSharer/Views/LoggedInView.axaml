<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:RecipeShare.ViewModels"
             x:DataType="vm:LoggedInViewModel"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
               x:Class="RecipeShare.Views.LoggedInView">
  <Grid ColumnDefinitions="200, *">
        <Border Background="#333" Grid.Column="0">
            <StackPanel VerticalAlignment="Top">
                <Button Content="Recipe" Command="{Binding ShowRecipeCommand}" Margin="10" />
                <Button Content="Rating" Command="{Binding ShowRatingCommand}" Margin="10" />
                <Button Content="Search" Command="{Binding ShowSearchCommand}" Margin="10" />
                <Button Content="Make A Recipe" Command="{Binding ShowMakeRecipeCommand}" Margin="10" />
                <Button Content="Profile" Command="{Binding ShowProfileCommand}" Margin="10" />
                <Button Content="Log Out" Command="{Binding Logout}" Margin="10" />
            </StackPanel>
        </Border>
    </Grid>
</UserControl>
